<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nxtele.did.vosDao.VosECdrDao">
    <select id="getAllCdr" resultType="com.nxtele.did.entity.VosECdr">
        select * from e_cdr_20200728
    </select>

    <select id="getVosCDRByTimeAndTableName" resultType="com.nxtele.did.entity.VosECdr">
        select *
        from ${tableName}
        where starttime between #{startTime} and #{endTime}
    </select>
</mapper>